<?xml version="1.0" ?><xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/cs02/xliff-core-1.2-strict.xsd">
    <file datatype="plaintext" original="languages/en/tl_news_archive.xlf" source-language="en" target-language="de">
        <body>
            <trans-unit id="tl_news_archive.master.0">
                <source>Master archive</source>
                <target>Hauptarchiv</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.master.1">
                <source>Please define the master archive to allow language switching.</source>
                <target>Wählen Sie das Hauptarchiv um Sprachenwechsel zu ermöglichen.</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.master.2">
                <source>This master has already been selected for the &quot;%s&quot; archive!</source>
                <target>Dies ist bereits das Hauptarchiv für &quot;%s&quot;!</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.language.0">
                <source>Language</source>
                <target>Sprache</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.language.1">
                <source>Please enter the page language according to the ISO-639-1 standard (e.g. &quot;en&quot; for English or &quot;en-US&quot; for American English).</source>
                <target>Bitte geben Sie die Sprache gemäss ISO-639-1 Standard ein (z.B. &quot;en&quot; für Englisch oder &quot;en-US&quot; für amerikanisches Englisch).</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.language_legend">
                <source>Language settings</source>
                <target>Spracheinstellungen</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.isMaster">
                <source>This is a master archive</source>
                <target>Dies ist ein Hauptarchiv</target>
            </trans-unit>
            <trans-unit id="tl_news_archive.isSlave">
                <source>Master archive is &quot;%s&quot;</source>
                <target>Das Hauptarchiv ist &quot;%s&quot;</target>
            </trans-unit>
        </body>
    </file>
